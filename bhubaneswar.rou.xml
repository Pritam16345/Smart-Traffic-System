<routes>
    <vType id="car_sedan" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="50" guiShape="passenger/sedan" color="1,0,0"/>
    <vType id="car_hatchback" accel="2.6" decel="4.5" sigma="0.5" length="4.5" maxSpeed="50" guiShape="passenger/hatchback" color="0,0,1"/>
    <vType id="truck" accel="1.5" decel="3.0" sigma="0.5" length="12" maxSpeed="40" guiShape="truck" color="0.5,0.5,0.5"/>
    <vType id="bus" accel="1.2" decel="2.5" sigma="0.5" length="15" maxSpeed="35" guiShape="bus" color="0,1,0"/>

    <route id="north_to_south" edges="N_to_C C_to_S"/>
    <route id="west_to_east" edges="W_to_C C_to_E"/>
    <route id="south_to_north" edges="S_to_C C_to_N"/>
    <route id="east_to_west" edges="E_to_C C_to_W"/>


    <flow id="flow_ns_sedan" type="car_sedan" route="north_to_south" begin="0" end="3600" probability="0.1"/>
    <flow id="flow_ns_hatchback" type="car_hatchback" route="north_to_south" begin="0" end="3600" probability="0.05"/>

    <flow id="flow_we_sedan" type="car_sedan" route="west_to_east" begin="0" end="3600" probability="0.1"/>
    <flow id="flow_we_hatchback" type="car_hatchback" route="west_to_east" begin="0" end="3600" probability="0.05"/>
    <flow id="flow_we_truck" type="truck" route="west_to_east" begin="0" end="3600" probability="0.02"/>

    <flow id="flow_sn_bus" type="bus" route="south_to_north" begin="0" end="3600" probability="0.03"/>
    <flow id="flow_sn_sedan" type="car_sedan" route="south_to_north" begin="0" end="3600" probability="0.08"/>

    <flow id="flow_ew_hatchback" type="car_hatchback" route="east_to_west" begin="0" end="3600" probability="0.12"/>

</routes>